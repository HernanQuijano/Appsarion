{
  "expo": {
    "name": "Appsarion",
    "slug": "appsarion",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/Logo.png",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appsarion.app"
    },
    "android": {
      "icon": "./assets/Logo.png",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.appsarion.app"
    },
    "updates": {
      "enabled": false
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos and videos."
        }
      ],
      "expo-font"
    ],
    "web": {
      "favicon": "./assets/Logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "3ab8a640-b04d-4e27-a131-3f812cd1221e"
      }
    }
  }
}
